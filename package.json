{
  "name": "big-2",
  "version": "1.0.1",
  "description": "Big 2",
  "author": "Catherine Han",
  "license": "",
  "private": true,
  "proxy": "http://localhost:3001",
  "scripts": {
    "server-dev": "python3 backend/manage.py runserver 0:3001",
    "server-prod": "gunicorn --chdir backend backend.wsgi",
    "migrate": "python3 backend/manage.py migrate",
    "sass-dev": "sass --watch src:src/styles --sourcemap=none",
    "test-backend": "python3 backend/manage.py test backend -v 2",
    "training": "python3 backend/api/training.py",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "8.10.0",
    "npm": "6.0.1"
  },
  "dependencies": {
    "ajv": "^6.0.0",
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0",
    "deck-of-cards": "^0.1.8",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "reactstrap": "^5.0.0-beta.3",
    "redux": "^3.7.2",
    "shortid": "^2.2.8",
    "styled-components": "^3.2.6"
  }
}
